		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/heatmap.js</footer-portlet-javascript>
	</portlet>
	<portlet>
		<portlet-name>gallery-visits</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>gallery-visits-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>exibit-visits</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>exibit-visits-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>Gallery Timeline</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>/js/jquery-1.11.0.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/highcharts.js</header-portlet-javascript>
		<header-portlet-javascript>/js/exporting.js</header-portlet-javascript>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>gallery-timeline-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
